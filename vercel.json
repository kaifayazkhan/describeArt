{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "headers": {
        "Cache-Control": "no-store, must-revalidate"
      },
      "continue": true,
      "timeout": 30
    }
  ]
}
